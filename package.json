    "dumi": "^1.1.7",
    "lerna": "^3.22.1",
    "less": "^4.1.1",
    "typescript": "^4.1.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.16",
